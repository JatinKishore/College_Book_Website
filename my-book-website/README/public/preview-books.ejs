<!DOCTYPE html>
<html>
<head>
  <title>Preview Books</title>
  <style>
    /* Add your custom styling here */
    table {
      border-collapse: collapse;
      width: 100%;
    }

    th, td {
      border: 1px solid black;
      padding: 8px;
      text-align: left;
    }

    th {
      background-color: #f2f2f2;
    }
  </style>
</head>
<body>
  <table>
    <tr>
      <th>Name</th>
      <th>Title</th>
      <th>Author Position</th>
      <th>Publication Date</th>
      <th>Publisher Name</th>
      <th>URL</th>
      <th>Remarks</th>
      <th>Book Type</th>
    </tr>
    <% books.forEach(book => { %>
    <tr>
      <td><%= book.name %></td>
      <td><%= book.title %></td>
      <td><%= book.authorPosition %></td>
      <td><%= book.publicationDate %></td>
      <td><%= book.publisherName %></td>
      <td><%= book.url %></td>
      <td><%= book.remarks %></td>
      <td><%= book.bookType %></td>
    </tr>
    <% }); %>
  </table>
</body>
</html>
